<h1>New User Sign Up</h1>
<%=link_to "sign up", new_user_path%>

<%if flash[:notice]%>
<%=flash[:notice]%>
<%end%>

<h1>Old User Sign In</h1>
<%=form_tag sessions_path method: "post" do%>
Name:<%=text_field_tag :'user[name]'%>
Password:<%=password_field_tag :'user[password]'%>
<%=submit_tag "Log In"%>
<%end%>
